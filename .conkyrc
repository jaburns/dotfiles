
background no
out_to_console yes
out_to_x no
update_interval 0.5
total_run_times 0
use_spacer none

lua_load ~/.conky.lua

TEXT
cpu ${lua_parse format %2u ${cpu cpu1}} \
${lua_parse format %2u ${cpu cpu2}} \
${lua_parse format %2u ${cpu cpu3}} \
${lua_parse format %2u ${cpu cpu4}} \
| ${lua_parse format %6.1f ${upspeedf}} up \
${lua_parse format %6.1f ${downspeedf}} down \
| ram used %${memperc} of ${memmax} \
