{
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/target/*/**": true,
        "**/msdf-atlas-gen/*/**": true
    },
    "files.exclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/target/*/**": true,
        "**/msdf-atlas-gen/*/**": true
    },
    "search.exclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/target/*/**": true,
        "**/msdf-atlas-gen/*/**": true
    },
    "workbench.colorTheme": "One Monokai 80s",
    "workbench.colorCustomizations": {
        "focusBorder": "#333842",
        "editorSuggestWidget.border": "#282c34",
        "tab.border": "#282C34",
        "panel.border": "#505762"
    },
    "keyboard.dispatch": "keyCode",
    "editor.parameterHints.enabled": false,
    "vim.useSystemClipboard": true,
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.fontFamily": "Consolas, 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "vim.insertModeKeyBindings": [
        {   "before": ["<c-v>"],
            "after": ["<esc>", "p", "a"]
        }
    ],
    "vim.visualModeKeyBindings": [
        {   "before": ["<c-j>"],
            "after": ["1","5","j"]
        },{
            "before": ["<c-k>"],
            "after": ["1","5","k"]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": ["j"],
            "after": ["g","j"]
        },{
            "before": ["k"],
            "after": ["g","k"]
        },{
            "before": ["<c-j>"],
            "after": ["1","5","j"]
        },{
            "before": ["<c-k>"],
            "after": ["1","5","k"]
        },{
            "before": ["<leader>", "i"],
            "after": [],
            "commands": [{
                "command": "editor.action.showHover",
                "args": []
            }]
        },{
            "before": ["<leader>", "s"],
            "after": [],
            "commands": [{
                "command": "editor.action.trimTrailingWhitespace",
                "args": []
            }]
        },{
            "before": ["<leader>","p"],
            "after": ["\"","x","d","i","w","P"]
        },{
            "before": ["Y"],
            "after": ["y","$"]
        },{
            "before": ["d","i","e"],
            "after": []
        },{
            "before": ["c","i","e"],
            "after": []
        },{
            "before": ["Z", "Z"],
            "after": [],
            "commands": [{
                "command": "workbench.action.files.save",
                "args": []
            },{
                "command": "workbench.action.closeActiveEditor",
                "args": []
            }]
        },{
            "before": ["Z", "Q"],
            "after": [],
            "commands": [{
                "command": "workbench.action.closeActiveEditor",
                "args": []
            }]
        },{
            "before": ["K"],
            "after": ["i","<cr>","<esc>"]
        }
    ],
    "editor.renderWhitespace": "boundary",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "csharp.suppressDotnetInstallWarning": true,
    "editor.fontSize": 12,
    "window.zoomLevel": 0,
    "terminal.integrated.fontSize": 12,
    "editor.minimap.enabled": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "explorer.confirmDragAndDrop": false,
    "editor.codeLens": false,
    "javascript.preferences.importModuleSpecifier": "relative",
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.detectIndentation": true,
    "rust-analyzer.lens.enable": false,
    "rust-analyzer.updates.askBeforeDownload": false,
    "rust-analyzer.inlayHints.parameterHints": false,
    "rust-analyzer.inlayHints.enable": false,
    "omnisharp.useGlobalMono": "always",
    "vim.leader": "<space>",
    "rust-analyzer.cargo.allFeatures": true,
    "workbench.editorAssociations": [],
    "explorer.confirmDelete": false,
    "rust-analyzer.cargo.loadOutDirsFromCheck": true,
    "rust-analyzer.procMacro.enable": true,
    "rust-analyzer.procMacro.server": null,
    "rust-analyzer.diagnostics.disabled": [
        "missing-unsafe"
    ],
    "[rust]": {
        "editor.formatOnSave": true
    },
    "glassit-linux.opacity": 90,
    "rust-analyzer.checkOnSave.command": "clippy",
}
